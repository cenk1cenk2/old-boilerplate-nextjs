{
  "name": "@kilic.dev/boilerplate-nextjs",
  "description": "Next.js boilerplate",
  "author": "Cenk Kilic",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf .next",
    "build": "next build",
    "start": "next start -p ${PORT:-'3000'}",
    "dev:start": "node --inspect=0.0.0.0:${DEBUG_PORT:-'9229'} node_modules/.bin/next -p ${PORT:-'3000'}",
    "lint": "yarn lint:staged src/",
    "lint:staged": "eslint --ext .json,.ts,.js,.tsx,.jsx --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "lint-staged": {
    "src/*.{ts,js,tsx,jsx,json}": [
      "yarn lint:staged"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@material-ui/core": "^4.9.14",
    "@material-ui/lab": "^4.0.0-alpha.53",
    "@types/lodash": "^4.14.151",
    "@types/styled-components": "^5.1.0",
    "@zeit/next-sass": "^1.0.1",
    "clsx": "^1.1.0",
    "config": "^3.3.1",
    "csstype": "^2.6.10",
    "deepmerge": "^4.2.2",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "next": "^11.1.0",
    "node-sass": "^4.14.1",
    "nprogress": "^0.2.0",
    "pjson": "^1.0.9",
    "react": "^16.13.1",
    "react-animations": "^1.0.0",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "styled-components": "^5.1.0",
    "typescript": "^3.9.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@cenk1cenk2/eslint-config": "0.2.5",
    "@testing-library/jest-dom": "^5.7.0",
    "@testing-library/react": "^10.0.4",
    "@testing-library/user-event": "^10.3.0",
    "@types/config": "^0.0.36",
    "@types/jest": "^25.2.2",
    "@types/material-ui": "^0.21.7",
    "@types/node": "^14.0.1",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/styled-jsx": "^2.2.8",
    "babel-plugin-styled-components": "^1.10.7",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "3.2.0",
    "eslint": "^7.0.0",
    "eslint-plugin-react": "^7.20.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "rimraf": "^3.0.2",
    "ts-essentials": "^6.0.5",
    "tsconfig-paths-webpack-plugin": "^3.2.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "files": [
    "/dist"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
